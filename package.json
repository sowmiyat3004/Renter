{
  "name": "renter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "prisma migrate deploy && next start",
    "start:dev": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "schema:lock": "node scripts/lock-schema.js lock",
    "schema:check": "node scripts/lock-schema.js check",
    "schema:unlock": "node scripts/lock-schema.js unlock",
    "schema:restore": "node scripts/lock-schema.js restore",
    "docker:build": "docker build -t renter-app .",
    "docker:run": "docker run -p 3000:3000 renter-app",
    "docker:compose": "docker-compose up -d",
    "dev:india": "NODE_ENV=development npm run dev",
    "build:india": "NODE_ENV=production npm run build",
    "setup:india": "node scripts/setup-india.js",
    "test:india": "npm test -- --testNamePattern=\"India\"",
    "netlify:build": "npm run db:generate && npm run build",
    "netlify:dev": "netlify dev"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.7.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.8.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/leaflet": "^1.9.8",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/react-datepicker": "^4.19.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "isomorphic-dompurify": "^2.28.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "multer": "^1.4.5-lts.1",
    "next": "^14.2.33",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.9.7",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "react": "^18",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.5.3",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-select": "^5.8.0",
    "sharp": "^0.33.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tsx": "^4.6.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}